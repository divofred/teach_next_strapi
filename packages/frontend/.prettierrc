{
  "printWidth": 100,
  "trailingComma": "es5",
  "singleQuote": true,
  "semi": false,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/src/api/(.*)$",
    "^@/src/components/(.*)$",
    "^@/src/lib/(.*)$",
    "^@/src/hooks/(.*)$",
    "^@/src/pages/(.*)$",
    "^@/src/store/(.*)$",
    "^@/src/constants/(.*)$",
    "^@/src/types/(.*)$",
    "^./(.*)",
    "^../(.*)",
    "(.scss)$",
    "(.css)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
